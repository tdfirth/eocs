/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=na);
    Not(in=b, out=nb);
    Or(a=a, b=b, out=r1);
    Or(a=na, b=nb, out=r2);
    And(a=r1, b=r2, out=out);
}